<section>
	<div class="container">
		<div class="row">
			<% if(notes){ %>
				<% notes.forEach((note) =>{ %>
					<a href="#">
						<div class="col-md-4 well" style="margin-right: 15px;">
							<h3><%= note.title %></h3>
							<p><%= note.description %></p>
						</div>
					</a>
				<% }) %>
			<%}%>
		</div>
	</div>
</section>