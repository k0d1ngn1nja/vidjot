<% if(errors && errors.length > 0){ %>
	<div class="row">
		<% errors.forEach((msg) =>{%>
			<div class="alert alert-danger alert-dismissable" style="border-radius: 0;">
				<button type="button" class="close" data-dismiss="alert" style="color: #fff;">&times;</button>
				<%= msg %>
			</div>
		<% }) %>
	</div><!--/row-->
<% } %>